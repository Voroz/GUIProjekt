<Window x:Class="GUIProjekt.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dev Team" Height="200" Width="250" MinHeight="200" MinWidth="250" Icon="images/memIcon.png">
    
    <Window.Background>
        <LinearGradientBrush StartPoint="0, 1" EndPoint="1, 0">
            <GradientStop Color="Black" Offset="0.0" />
            <GradientStop Color="White" Offset="1.0" />
        </LinearGradientBrush>
    </Window.Background>
    
    <Viewbox HorizontalAlignment="Left">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="30*"/>
                <RowDefinition Height="30*"/>
                <RowDefinition Height="30*"/>
                <RowDefinition Height="30*"/>
                <RowDefinition Height="30*"/>
            </Grid.RowDefinitions>
            
            
            <Label Grid.Row="0" Foreground="BurlyWood">Jonny Fherm </Label>
            <Label Grid.Row="1" Foreground="BurlyWood">Gustav Halje</Label>
            <Label Grid.Row="2" Foreground="BurlyWood">Marcus Jakobsson</Label>
            <Label Grid.Row="3" Foreground="BurlyWood">Pontus Mansfeld</Label>
            <Label Grid.Row="4" Foreground="BurlyWood" FontStyle="Italic">DMP15 2017 JTH</Label>
        </Grid>
    </Viewbox>
</Window>
