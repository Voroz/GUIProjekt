<Window x:Class="GUIProjekt.Commands"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Commands" Height="300" Width="100" Icon="images/memIcon.png">
    <Grid>
        <Viewbox HorizontalAlignment="Left">
            <UniformGrid Rows="12">
                <Label Foreground="BurlyWood">LOAD [addr]</Label>
                <Label Foreground="BurlyWood">STORE [addr]</Label>
                <Label Foreground="BurlyWood">ADD [addr]</Label>
                <Label Foreground="BurlyWood">SUB [addr]</Label>
                <Label Foreground="BurlyWood">MUL [addr]</Label>
                <Label Foreground="BurlyWood">JUMP [addr]</Label>
                <Label Foreground="BurlyWood">PJUMP [addr]</Label>
                <Label Foreground="BurlyWood">IN</Label>
                <Label Foreground="BurlyWood">OUT</Label>
                <Label Foreground="BurlyWood">CALL [addr]</Label>
                <Label Foreground="BurlyWood">RETURN</Label>
                <Label Foreground="BurlyWood" FontStyle="Italic">:nameOfLabel</Label>
            </UniformGrid>            
        </Viewbox>
    </Grid>
</Window>
