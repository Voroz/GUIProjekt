<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="WindowBackground" TargetType="Window">
        <Setter Property="Background" Value="#293955"/>
    </Style>

    <!-- Menu -->
    <Brush x:Key="MenuColor">#d6dbe9</Brush>
    <Brush x:Key="MenuColor1">#d6dbe9</Brush>
    <Brush x:Key="MenuColor2">#d6dbe9</Brush>
    <Brush x:Key="fontColor">#000000</Brush>
    <Brush x:Key="fontColorFileMenu">#000000</Brush>

    <!-- IN, OUT, WR, IP Label Font color -->
    <Brush x:Key="programLabels">#FFFFFF</Brush>

    <!-- sliderFontColor-->
    <Brush x:Key="sliderFontColor">#000000</Brush>

    <!-- MEMORY ROW COLORS -->
    <Brush x:Key="rowNrColor">#ffffff</Brush>
    <Brush x:Key="BackgroundZeroColor">#d6dbe9</Brush>
    <Brush x:Key="BackgroundOneColor">#F7B32B</Brush>
    <Brush x:Key="ForegroundZeroColor">#000000</Brush>
    <Brush x:Key="ForegroundOneColor">#000000</Brush>

    <!-- Message Box -->
    <Brush x:Key="MessageBoxBackground">#FFFFFF</Brush>

    <!-- Text Box -->
    <Brush x:Key="TextBoxBackground">#FFFFFF</Brush>
    <Brush x:Key="TextBoxForegroundOff">#000000</Brush>
    <Brush x:Key="TextBoxForegroundOn">LightGray</Brush>
</ResourceDictionary>