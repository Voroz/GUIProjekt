<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="WindowBackground" TargetType="Window">
        <Setter Property="Background" Value="#293955"/>
    </Style>

    <!-- Menu -->
    <Brush x:Key="MenuColor">#d6dbe9</Brush> <!--Row #0-->
    <Brush x:Key="MenuColor1">#d6dbe9</Brush> <!--Row #1-->
    <Brush x:Key="MenuColor2">#d6dbe9</Brush> <!--Row #2-->
    <Brush x:Key="fontColor">#000000</Brush>
    <Brush x:Key="fontColorFileMenu">#000000</Brush>
    
    <!-- IN, OUT, WR, IP Label Font color -->
    <Brush x:Key="programLabels">#fff</Brush>

    <!-- sliderFontColor-->
    <Brush x:Key="sliderFontColor">#000000</Brush>

    <!-- MEMORY ROW COLORS -->
    <Brush x:Key="rowNrColor">#ffffff</Brush>
    <Brush x:Key="zeroColor">#d6dbe9</Brush>
    <Brush x:Key="oneColor">#F7B32B</Brush>
    
    <!-- Message Box -->
    <Brush x:Key="MessageBoxBackground">#FFFFFF</Brush>
    <Brush x:Key="MessageBoxForeground">#FFFFFF</Brush>
</ResourceDictionary>