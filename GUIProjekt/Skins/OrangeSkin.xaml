<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="WindowBackground" TargetType="Window">
        <Setter Property="Background" Value="#ffeab4"/>
    </Style>

    <Brush x:Key="MenuColor">#f98c13</Brush>    <!--Row #0-->
    <Brush x:Key="MenuColor1">#f98c13</Brush>    <!--Row #1-->
    <Brush x:Key="MenuColor2">#f98c13</Brush>    <!--Row #2-->
    <Brush x:Key="fontColor">#000000</Brush>
    <Brush x:Key="fontColorFileMenu">#000000</Brush>
    <Brush x:Key="toggleClicked">#000000</Brush>
    <Brush x:Key="toggleFont">#f98c13</Brush>

    <Brush x:Key="toggleClicked2">#FFFFFF</Brush>
    <Brush x:Key="toggleFont2">#000000</Brush>

    <!-- MEMORY ROW COLORS -->
    <Brush x:Key="zeroColor">#FFFF</Brush>
    <Brush x:Key="oneColor">#FF8E11</Brush>

</ResourceDictionary>