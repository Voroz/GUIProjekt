<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="WindowBackground" TargetType="Window">
        <Setter Property="Background" Value="#b2b8ba"/>
    </Style>

    <Brush x:Key="MenuColor">#4b5154</Brush>
    <!--Row #0-->
    <Brush x:Key="MenuColor1">#4b5154</Brush>
    <!--Row #1-->
    <Brush x:Key="MenuColor2">#8c182c</Brush>
    <!--Row #2-->
    <Brush x:Key="fontColor">#ffffff</Brush>
    <Brush x:Key="fontColorFileMenu">#000000</Brush>
    <Brush x:Key="toggleClicked">#000000</Brush>
    <Brush x:Key="toggleFont">#f98c13</Brush>
    <!-- IN, OUT, WR, IP Label Font color -->
    <Brush x:Key="programLabels">#000000</Brush>

    <!-- MEMORY ROW COLORS -->
    <Brush x:Key="rowNrColor">#000000</Brush>
    <Brush x:Key="toggleClicked2">#FFFFFF</Brush>
    <Brush x:Key="toggleFont2">#000000</Brush>

    <!-- MEMORY ROW COLORS -->
    <Brush x:Key="zeroColor">#788385</Brush>
    <Brush x:Key="oneColor">#ce160e</Brush>

    <!-- Message Box -->
    <Brush x:Key="MessageBoxBackground">#FFFFFF</Brush>
    <Brush x:Key="MessageBoxForeground">#FFFFFF</Brush>
</ResourceDictionary>