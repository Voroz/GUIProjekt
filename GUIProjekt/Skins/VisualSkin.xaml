<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="WindowBackground" TargetType="Window">
        <Setter Property="Background" Value="#c6cbcc"/>
    </Style>
   
    <Brush x:Key="MenuColor">#33484c</Brush> 
    <Brush x:Key="MenuColor1">#33484c</Brush>  
    <Brush x:Key="MenuColor2">#3a4866</Brush>
    <Brush x:Key="fontColor">#FFFF</Brush>
    <Brush x:Key="fontColorFileMenu">#000000</Brush>
    <Brush x:Key="toggleClicked">#FFFFFF</Brush>
    <Brush x:Key="toggleFont">#000000</Brush>
    <Brush x:Key="toggleClicked2">#7f7062</Brush>
    <Brush x:Key="toggleFont2">#FFFFFF</Brush>

    <!-- IN, OUT, WR, IP Label Font color -->
    <Brush x:Key="programLabels">#000000</Brush>

    <Brush x:Key="sliderFontColor">#FFFFFF</Brush>

    <!-- MEMORY ROW COLORS -->
    <Brush x:Key="rowNrColor">#000000</Brush>
    <Brush x:Key="zeroColor">#3A4866</Brush>
    <Brush x:Key="oneColor">#7F7062</Brush>

    <!-- Message Box -->
    <Brush x:Key="MessageBoxBackground">#FFFFFF</Brush>
    <Brush x:Key="MessageBoxForeground">#FFFFFF</Brush>
</ResourceDictionary>