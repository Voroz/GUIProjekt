<Window x:Class="GUIProjekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:GUIProjekt"
        Title="Modelldatorn Assembler" MinHeight="600" MinWidth="800" Height="768" Width="1024">
    
    <Window.Resources>
        <!-- Mall för menyknapparna -->
        <Style x:Key="MenuButton" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="Padding" Value="10" />
            <Setter Property="ContentTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Viewbox>
                            <ContentPresenter Content="{TemplateBinding Content}" />
                        </Viewbox>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!-- Bakgrundsfärg för hela menyområdet -->
        <Brush x:Key="MenuColor">#eeeeee</Brush>
    </Window.Resources>
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"></RowDefinition>
            <RowDefinition MinHeight="25" MaxHeight="45" /> <!-- Meny -->
            <RowDefinition Height="10*" />  <!-- Content -->
        </Grid.RowDefinitions>
        
        <Menu Grid.Row="0" Width="Auto" Height="20" Background="{StaticResource MenuColor}">   <!-- Topmeny/Standardmeny -->
            <MenuItem Header="_File">
                <MenuItem Header="Open"></MenuItem>
                <MenuItem Header="Save"></MenuItem>
                <MenuItem Header="Exit"></MenuItem>
            </MenuItem>
            <MenuItem Header="Help">
                <MenuItem Header="About"></MenuItem>
            </MenuItem>
        </Menu>
        
        <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Stretch" Background="{StaticResource MenuColor}">
            <Button Click="Button_Run_Click" Style="{StaticResource MenuButton}" ToolTip="Play">
                <Image Source="images/media-play-8x.png" />
            </Button>
            <Button Style="{StaticResource MenuButton}" ToolTip="Pause">
                <Image Source="images/media-pause-8x.png" />
            </Button>
            <Button Name="Button_Stop" Click="Button_Stop_Click" Style="{StaticResource MenuButton}" ToolTip="Stop">
                <Image Source="images/media-stop-8x.png" />
            </Button>
            <Button Style="{StaticResource MenuButton}" ToolTip="Step Forward">
                <Image Source="images/action-undo-8x.png" />
            </Button>
            <Button Style="{StaticResource MenuButton}" ToolTip="Step Back">
                <Image Source="images/action-redo-8x.png" />
                <ToolTipService.ToolTip>
                    <TextBlock Text="Step Back" />
                </ToolTipService.ToolTip>
            </Button>
            
            <Viewbox>
                <ToggleButton Name="PlayButton" Background="Lavender" BorderBrush="LightBlue">
                    <Image Source="images/play-button.png" Height="9" Width="9" ToolTip="Play"></Image>
                </ToggleButton>
            </Viewbox>
            <Viewbox>
                <ToggleButton Name="PauseButton" Background="Lavender" BorderBrush="LightBlue">
                    <Image Source="images/pause.png" Height="10" Width="10" ToolTip="Pause"></Image>
                </ToggleButton>
            </Viewbox>
            <Viewbox>
                <ToggleButton Name="StopButton" Background="Lavender" BorderBrush="LightBlue">
                    <Image Source="images/Stopbtn.png" Height="10" Width="10" ToolTip="Stop"></Image>
                </ToggleButton>
            </Viewbox>
            
        </StackPanel>
        

        <Grid Grid.Row="2" Margin="10,35,10,10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*" /> 
                <ColumnDefinition Width="1*" /> 
                <ColumnDefinition Width="2*" /> 
            </Grid.ColumnDefinitions>

            <!--Minnet --> 
