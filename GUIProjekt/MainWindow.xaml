<Window x:Class="GUIProjekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:GUIProjekt"
        Title="Modelldatorn Assembler" Style="{DynamicResource WindowBackground}" MinHeight="600" MinWidth="800" Height="768" Width="1024">
    <!--<Window.Background>
        <VisualBrush TileMode="Tile" Viewport="0,0,1,1">
            <VisualBrush.Visual>
                
                <Image Source="images/backg.jpg" Stretch="Fill"></Image>
            </VisualBrush.Visual>
        </VisualBrush>
    </Window.Background>-->

    <Window.Resources>
        <!-- Mall för layouten av programmet hittar ni under skins, Skapa egna skins för att ändra färger etc -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Skins/DefaultSkin.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        <!--Resources som inte ska tillhöra layout förändring läggs här-->    
            <local:FontSizeConverter x:Key="fontSizeCon"/>
        </ResourceDictionary>               
    </Window.Resources>
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="3*" /> <!-- Meny -->
            <RowDefinition Height="2*" />
            <RowDefinition Height="50*" />  <!-- Content -->
            <RowDefinition Height="20*" />  <!--Stack-->
        </Grid.RowDefinitions>
        
        <Menu Grid.Row="0" Width="Auto" Height="30" Background="{DynamicResource MenuColor}" FontSize="16">   <!-- Topmeny/Standardmeny -->
            <MenuItem Header="_File">
                <MenuItem Name="Open" Header="Open" Click="Open_Click"></MenuItem>
                <MenuItem Name="Save" Header="Save" Click="Save_Click"></MenuItem>
                <MenuItem Header="Exit"></MenuItem>
            </MenuItem>
            <MenuItem Header="Help">
                <MenuItem Header="About"></MenuItem>
            </MenuItem>
        </Menu>
        
        <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Stretch" Background="{DynamicResource MenuColor}">
            <Viewbox>
                <Button Click="Button_Run_Click" Style="{DynamicResource MenuButton}" ToolTip="Play">
                    <Image Source="images/media-play-8x.png" />
                </Button>
            </Viewbox>
            <Viewbox>
                <Button Click="Button_Pause_Click" Style="{DynamicResource MenuButton}" ToolTip="Pause">
                    <Image Source="images/media-pause-8x.png" />
                </Button>
            </Viewbox>
            <Viewbox>
                <Button Name="Button_Stop" Click="Button_Stop_Click" Style="{DynamicResource MenuButton}" ToolTip="Stop">
                    <Image Source="images/media-stop-8x.png" />
                </Button>
            </Viewbox>
            
            <Viewbox>
                <Button Name="Button_StepBack" Click="Button_StepBack_Click" Style="{DynamicResource MenuButton}" ToolTip="Step Back">
                    <Image Source="images/action-undo-8x.png" />
                    <ToolTipService.ToolTip>
                        <TextBlock Text="Step Back" />
                    </ToolTipService.ToolTip>
                </Button>
            </Viewbox>

            <Viewbox>
                <Button Name="Button_StepForward" Click="Button_StepForward_Click" Style="{DynamicResource MenuButton}" ToolTip="Step Forward">
                    <Image Source="images/action-redo-8x.png" />
                </Button>
            </Viewbox>
            

            <!--<Viewbox>
                <ToggleButton Name="PlayButton" Background="#DDDDE4" BorderBrush="#DDDDE4">
                    <Image Source="images/play-button.png" Height="9" Width="9" ToolTip="Play"></Image>
                </ToggleButton>
            </Viewbox>
            <Viewbox>
                <ToggleButton Name="PauseButton" Click="PauseButton_Click" Background="#DDDDE4" BorderBrush="#DDDDE4">
                    <Image Source="images/pause.png" Height="10" Width="10" ToolTip="Pause"></Image>
                </ToggleButton>
            </Viewbox>
            <Viewbox>
                <ToggleButton Name="StopButton" Background="#DDDDE4" BorderBrush="#DDDDE4">
                    <Image Source="images/Stopbtn.png" Height="10" Width="10" ToolTip="Stop"></Image>
                </ToggleButton>
            </Viewbox>-->
            <Viewbox>
                <ComboBox Text="Is not open" Background="{DynamicResource MenuColor}">
                    <ComboBoxItem MouseMove="combBoxDefault_MouseMove" Name="combBoxDefault">Default</ComboBoxItem>
                    <ComboBoxItem MouseMove="combBoxOrange_MouseMove"  Name="combBoxOrange">Orange</ComboBoxItem>
                    <ComboBoxItem MouseMove="combBoxBlue_MouseMove" Name="combBoxBlue">Blue</ComboBoxItem>
                </ComboBox>
            </Viewbox>
            
        </StackPanel>
        
        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="47*" />
                <ColumnDefinition Width="5*" />
                <ColumnDefinition Width="10.6*" />
                <ColumnDefinition Width="22*" />           
                <ColumnDefinition Width="51*" />
                <ColumnDefinition Width="4*" />
            </Grid.ColumnDefinitions>
            <Label Background="{DynamicResource MenuColor}" Grid.Column="0"></Label>
            <Label Background="{DynamicResource MenuColor}" Grid.Column="1"></Label>
            <Label Background="{DynamicResource MenuColor}" Grid.Column="2"></Label>
            <Label Background="{DynamicResource MenuColor}" Grid.Column="3"></Label>
            <!-- TODO: Visa vad som är Operationer / Värde i minnet -->
            <Viewbox Grid.Column="1" Stretch="Uniform" HorizontalAlignment="Center">
                <Label Background="{DynamicResource MenuColor}" FontSize="25">Row</Label>
            </Viewbox>
            <Viewbox Grid.Column="2" Stretch="Uniform" HorizontalAlignment="Center">
                <Label Background="{DynamicResource MenuColor}" Grid.Column="2" FontSize="25">Operation</Label>
            </Viewbox>
            <Viewbox Grid.Column="3" Stretch="Uniform" HorizontalAlignment="Center">
                <Label Background="{DynamicResource MenuColor}" Grid.Column="3" FontSize="25">Value</Label>
            </Viewbox>
            
                 
            <UniformGrid Columns="3" Grid.Column="4" >
                <RadioButton Style="{DynamicResource EditorTabs}" Content="Machine Code" Name="MKTab" Click="TabsToggle_event" />
                <RadioButton Style="{DynamicResource EditorTabs}" Content="Split View" Name="SplitTab" Click="TabsToggle_event" IsChecked="True" />
                <RadioButton Style="{DynamicResource EditorTabs}" Content="Assembly" Name="AssemblyTab" Click="TabsToggle_event"/>
            </UniformGrid>
            <Label Background="{DynamicResource MenuColor}" Grid.Column="6" /> <!-- För att fylla ut tomrummet -->
        </Grid>

